inhibit_all_warnings!

# Uncomment the next line to define a global platform for your project
platform :ios, '11.3'

target 'easyNotesiOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for easyNotesiOS
  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Messaging'
  pod 'FirebaseUI'
  pod 'Alamofire'

target 'easyNotesiOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'easyNotesiOSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
